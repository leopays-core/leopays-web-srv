{
  "env": "production",
  "data": {
    "dir": "/data"
  },
  "config": {
    "file": "config.json"
  },
  "links": {
    "url": "https://leopays.dev/"
  },
  "logger": {
    "level": "trace",
    "appenders": {
      "stdout": true,
      "stderr": false,
      "gelf": false,
      "file": true
    },
    "file": {
      "name": "debug.log",
      "pattern": ".yyyy-MM-dd-hh",
      "keep_file_ext": true,
      "encoding": "utf-8",
      "mode": 511,
      "flags": "a",
      "compress": false,
      "always_include_pattern": false,
      "days_to_keep": 1
    }
  },
  "server": {
    "secure_mode": false,
    "host": "::",
    "exclusive": false,
    "ipv6Only": false,
    "http": {
      "port": 80
    },
    "https": {
      "port": 443,
      "key": "privkey.pem",
      "cert": "cert.pem",
      "ca": "chain.pem"
    },
    "https_redirect": false,
    "www_redirect": false,
    "without_www_redirect": false
  }
}